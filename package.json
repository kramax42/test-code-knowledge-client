{
  "name": "med-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npx json-server --port 3004 --watch src/data/questions.json\" \"next dev\"",
    "export": "next export",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint:js": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:js:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "lint:css": "stylelint **/*.{scss,css}",
    "lint:css:fix": "stylelint --fix **/*.{scss,css}",
    "lint:json": "prettier --check \"./**/*.json\"",
    "lint:json:fix": "prettier --write \"./**/*.json\"",
    "lint": "npm run lint:js && npm run lint:css && npm run lint:json",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix && npm run lint:json:fix"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.7.1",
    "axios": "^0.24.0",
    "clsx": "^1.1.1",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "date-fns": "^2.28.0",
    "next": "12.0.1",
    "prism-react-renderer": "^1.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^7.2.6",
    "redux-persist": "^6.0.0",
    "sass": "^1.32.6",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@svgr/webpack": "^6.1.2",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "eslint-config-next": "^12.1.6",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "prettier": "^2.6.2",
    "stylelint": "^14.8.2",
    "stylelint-checkstyle-formatter": "^0.1.2",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-scss": "^4.2.0",
    "typescript": "^4.5.2"
  }
}
